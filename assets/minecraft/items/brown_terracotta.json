{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylonbase:fluid_pipe_wood",
        "model": {
          "type": "model",
          "model": "pylonbase:block/machines/pipes/wood_pipe"
        }
      },
      {
        "when": "pylonbase:hydraulic_press_piston",
        "model": {
          "type": "model",
          "model": "pylonbase:block/machines/hydraulic/press_piston"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "block/brown_terracotta"
    }
  }
}