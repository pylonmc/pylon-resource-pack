{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "rebar:toggle-armor-textures_enabled",
        "model": {
          "type": "model",
          "model": "rebar:item/ui/armor_texture"
        }
      },
      {
        "when": "pylon:sugarcane",
        "model": {
          "type": "model",
          "model": "pylon:block/fluids/water",
          "tints": [
            {
              "type": "constant",
              "value": 13290377
            }
          ]
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "block/lime_concrete"
    }
  }
}