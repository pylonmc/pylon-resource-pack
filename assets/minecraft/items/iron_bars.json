{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:filter_mesh",
        "model": {
          "type": "model",
          "model": "pylon:item/components/filter_mesh"
        }
      },
      {
        "when": "pylon:sawblade",
        "model": {
          "type": "model",
          "model": "pylon:item/components/sawblade"
        }
      },
      {
        "when": "pylon:hydraulic_table_saw:saw",
        "model": {
          "type": "model",
          "model": "pylon:item/components/air"
        }
      },
      {
        "when": "pylon:diesel_table_saw:saw",
        "model": {
          "type": "model",
          "model": "pylon:item/components/air"
        }
      },
      {
        "when": "pylon:diesel_table_saw",
        "model": {
          "type": "model",
          "model": "pylon:block/machines/diesel/table_saw_north"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/iron_bars"
    }
  }
}