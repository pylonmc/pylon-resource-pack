{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases":[
      {
        "when": "pylon:ice_arrow",
        "model": {
          "type": "model",
          "model": "pylon:item/combat/ice_arrow"
        }
      },
      {
        "when": "pylon:recoil_arrow",
        "model": {
          "type": "model",
          "model": "pylon:item/combat/recoil_arrow"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/arrow"
    }
  }
}