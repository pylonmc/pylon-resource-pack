{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:explosive_target",
        "model": {
          "type": "model",
          "model": "pylon:block/building/explosive_target"
        }
      },
      {
        "when": "pylon:explosive_target_fiery",
        "model": {
          "type": "model",
          "model": "pylon:block/building/fiery_explosive_target"
        }
      },
      {
        "when": "pylon:explosive_target_super",
        "model": {
          "type": "model",
          "model": "pylon:block/building/super_explosive_target"
        }
      },
      {
        "when": "pylon:explosive_target_super_fiery",
        "model": {
          "type": "model",
          "model": "pylon:block/building/super_fiery_explosive_target"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "block/target"
    }
  }
}