{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylonbase:portable_dustbin",
        "model": {
          "type": "model",
          "model": "pylonbase:item/tools/portable_dustbin"
        }
      },
      {
        "when": "pylonbase:mixing_pot",
        "model": {
          "type": "model",
          "model": "pylonbase:block/machines/simple_machines/mixing_pot_north"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/cauldron"
    }
  }
}