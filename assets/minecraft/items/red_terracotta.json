{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:iron",
        "model": {
          "type": "model",
          "model": "pylon:block/fluids/iron"
        }
      },
      {
        "when": "pylon:cargo_extractor",
        "model": {
          "type": "model",
          "model": "pylon:block/machines/cargo/cargo_extractor_north"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "block/red_terracotta"
    }
  }
}