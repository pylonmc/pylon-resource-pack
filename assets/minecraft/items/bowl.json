{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:farming_talisman_simple",
        "model": {
          "type": "model",
          "model": "pylon:item/talismans/farming_talisman_simple"
        }
      },
      {
        "when": "pylon:farming_talisman_advanced",
        "model": {
          "type": "model",
          "model": "pylon:item/talismans/farming_talisman_advanced"
        }
      },
      {
        "when": "pylon:farming_talisman_ultimate",
        "model": {
          "type": "model",
          "model": "pylon:item/talismans/farming_talisman_ultimate"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/bowl"
    }
  }
}