{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:sulfur",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/sulfur"
        }
      },
      {
        "when": "pylon:dough",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/dough"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/yellow_dye"
    }
  }
}