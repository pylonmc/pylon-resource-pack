              }
            },
            {
              "when": "resin",
              "model": {
                "type": "model",
                "model": "item/bronze_leggings_resin_trim"
              }
            }
          ],
          "fallback": {
            "type": "model",
            "model": "pylon:item/armor/bronze_leggings"
