{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylonbase:health_talisman_simple",
        "model": {
          "type": "model",
          "model": "pylonbase:item/combat/health_talisman_simple"
        }
      },
      {
        "when": "pylonbase:portability_catalyst",
        "model": {
          "type": "model",
          "model": "pylonbase:item/components/portability_catalyst"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/amethyst_shard"
    }
  }
}