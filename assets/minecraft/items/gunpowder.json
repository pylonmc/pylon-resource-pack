{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "pylon:rock_dust",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/rock_dust"
        }
      },
      {
        "when": "pylon:coal_dust",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/coal_dust"
        }
      },
      {
        "when": "pylon:iron_dust",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/iron_dust"
        }
      },
      {
        "when": "pylon:steel_dust",
        "model": {
          "type": "model",
          "model": "pylon:item/resources/steel_dust"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/gunpowder"
    }
  }
}