{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "cases": [
      {
        "when": "rebar:guide_page:rebar:settings",
        "model": {
          "type": "model",
          "model": "rebar:item/ui/settings"
        }
      },
      {
        "when": "pylon:cargo_filter",
        "model": {
          "type": "model",
          "model": "pylon:block/machines/cargo/cargo_filter_north"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "item/comparator"
    }
  },
  "oversized_in_gui": true
}